<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.16.0 -->
<interface>
  <!-- interface-name latexeditor.ui -->
  <requires lib="gtk" version="4.12"/>
  <requires lib="libadwaita" version="1.0"/>
  <template class="RnLatexEditor" parent="AdwApplicationWindow">
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="AdwHeaderBar">
            <child>
              <object class="GtkButton" id="compile_button">
                <property name="label">Compile!</property>
                <style>
                  <class name="suggested-action"/>
                </style>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkScrolledWindow">
            <property name="hexpand">True</property>
            <property name="vexpand">True</property>
            <child>
              <object class="GtkTextView">
                <property name="buffer">
                  <object class="GtkTextBuffer" id="latex_code"/>
                </property>
                <property name="hscroll-policy">natural</property>
                <property name="vexpand">True</property>
                <property name="vscroll-policy">natural</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
